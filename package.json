{
  "name": "Corona-Ampel-Coburg",
  "description": "Corona-Ampel-Coburg",
  "version": "2.1.5",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy --concurrent 1",
    "start": "node __sapper__/build",
    "data": "node --experimental-json-modules ./scripts/fetch-data.mjs",
    "version": "npm version patch && node --experimental-json-modules ./scripts/write-version.mjs",
    "prettier": "prettier --write .",
    "lint:css": "stylelint **/*.{css,html,js,md,mjs,svelte,svg,ts} --fix"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0",
    "svelte-chartjs": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.1",
    "stylelint": "^13.7.1",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-high-performance-animation": "^1.5.1",
    "stylelint-images": "^2.0.1",
    "stylelint-scss": "^3.18.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "moment": "^2.29.1",
    "svelte": "^3.29.4"
  },
  "lint-staged": {
    "*.{svelte,js,css,md}": "prettier --write"
  },
  "ignore": [
    "node_modules",
    "static",
    "__sapper__",
    "src/node_modules/@sapper",
    "package-lock.json"
  ]
}
